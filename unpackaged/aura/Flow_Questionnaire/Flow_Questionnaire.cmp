<aura:component controller="flowQuestionnaireController" implements="lightning:availableForFlowScreens,force:hasRecordId" access="global" >
	<aura:handler name="init" value="{!this}" action="{!c.init}" />
    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="questionnaireId" type="String" />
    <aura:attribute name="category" type="String" />
    <aura:attribute name="requiredQuestionsAnswered" type="Boolean" default="false"/>
    <aura:attribute name="booleanTest" type="Boolean" default="false"/>
    <aura:attribute name="questions" type="Object[]"/>
    <aura:attribute name="options" type="List" default="[]"/>
    <aura:attribute name="responses" type="Q_A__c[]" default="[]"/>
    <aura:attribute name="response" type="Q_A__c" default="{}"/>

    {!v.body}
                    
</aura:component>