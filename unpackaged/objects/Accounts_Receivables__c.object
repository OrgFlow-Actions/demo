    <externalId>false</externalId>
    <formula>IF( ESRECO_Code__c =0,  IF( TODAY() &gt; Due_Date__c ,"YES", "NO") ,  IF( Last_Payment_Date__c  &gt;  Due_Date__c , "YES", "NO") )</formula>
