  <deploymentStatus>Deployed</deploymentStatus>
  <description>Custom object used to link the case to multiple accounts with different roles. The standard Case Contact Role object is a legacy and has many limitation</description>
  <enableActivities>false</enableActivities>
  <enableBulkApi>true</enableBulkApi>
  <enableFeeds>false</enableFeeds>
  <enableHistory>false</enableHistory>
  <enableLicensing>false</enableLicensing>
  <enableReports>true</enableReports>
  <enableSearch>false</enableSearch>
  <enableSharing>true</enableSharing>
  <enableStreamingApi>true</enableStreamingApi>
  <externalSharingModel>ControlledByParent</externalSharingModel>
  <fields>
    <fullName>Account__c</fullName>
    <externalId>false</externalId>
    <label>Account</label>
    <referenceTo>Account</referenceTo>
    <relationshipLabel>Related Cases</relationshipLabel>
    <relationshipName>CaseAccount</relationshipName>
    <relationshipOrder>0</relationshipOrder>
    <reparentableMasterDetail>true</reparentableMasterDetail>
    <trackTrending>false</trackTrending>
    <type>MasterDetail</type>
    <writeRequiresMasterRead>true</writeRequiresMasterRead>
  </fields>
  <fields>
    <fullName>Case__c</fullName>
    <externalId>false</externalId>
    <label>Case</label>
    <referenceTo>Case</referenceTo>
    <relationshipLabel>Related Accounts</relationshipLabel>
    <relationshipName>CaseAccountRoles</relationshipName>
    <relationshipOrder>1</relationshipOrder>
    <reparentableMasterDetail>true</reparentableMasterDetail>
    <trackTrending>false</trackTrending>
    <type>MasterDetail</type>
    <writeRequiresMasterRead>true</writeRequiresMasterRead>
  </fields>
  <fields>
    <fullName>Contact__c</fullName>
    <deleteConstraint>SetNull</deleteConstraint>
    <externalId>false</externalId>
    <label>Contact</label>
    <lookupFilter>
      <active>true</active>
      <filterItems>
        <field>Contact.AccountId</field>
        <operation>equals</operation>
        <valueField>$Source.Account__c</valueField>
      </filterItems>
      <isOptional>true</isOptional>
    </lookupFilter>
    <referenceTo>Contact</referenceTo>
    <relationshipLabel>Related Cases</relationshipLabel>
    <relationshipName>CaseContactRolesCustom</relationshipName>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Lookup</type>
  </fields>
  <fields>
    <fullName>Email__c</fullName>
    <description>Show Email from contact if populated, otherwise to show it from Account</description>
    <externalId>false</externalId>
    <formula>If(  Contact__c  ='', Account__r.General_Info_Email__c, Contact__r.Email  )</formula>
    <label>Email</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
  </fields>
  <fields>
    <fullName>MainContactPoint__c</fullName>
    <defaultValue>false</defaultValue>
    <description>The primary account/contact for all communications around this case</description>
    <externalId>false</externalId>
    <inlineHelpText>The primary account/contact for all communications around this case</inlineHelpText>
    <label>Main Contact Point</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
  </fields>
  <fields>
    <fullName>Phone__c</fullName>
    <description>Show the phone number from Contact if populated, otherwise from Account Phone</description>
    <externalId>false</externalId>
    <formula>If(  Contact__c  ='', Account__r.Phone , Contact__r.Phone )</formula>
    <label>Phone</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
  </fields>
  <fields>
    <fullName>Role__c</fullName>
    <description>Account/Contact role for this Case</description>
    <externalId>false</externalId>
    <label>Role</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
      <restricted>true</restricted>
      <valueSetDefinition>
        <sorted>true</sorted>
        <value>
          <fullName>Dealer</fullName>
          <default>false</default>
          <label>Dealer</label>
        </value>
        <value>
          <fullName>Dentist</fullName>
          <default>false</default>
          <label>Dentist</label>
        </value>
        <value>
          <fullName>Lab</fullName>
          <default>false</default>
          <label>Lab</label>
        </value>
        <value>
          <fullName>Non-SFDC Internal Team</fullName>
          <default>false</default>
          <label>Non-SFDC Internal Team</label>
        </value>
        <value>
          <fullName>Payer</fullName>
          <default>false</default>
          <label>Payer</label>
        </value>
        <value>
          <fullName>Third Party Contractor</fullName>
          <default>false</default>
          <label>Third Party Contractor</label>
        </value>
      </valueSetDefinition>
    </valueSet>
  </fields>
  <label>Case Account Role</label>
  <nameField>
    <displayFormat>CAR{000000000}</displayFormat>
    <label>Case Account Role Name</label>
    <type>AutoNumber</type>
  </nameField>
