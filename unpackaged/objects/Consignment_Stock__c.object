  </fields>
  <fields>
    <fullName>Expiration_Date__c</fullName>
    <externalId>false</externalId>
    <formula>IF  (ISBLANK( LotRef1__c ), 
    null,  
    IF  (LEN(TRIM(LotRef1__c))=10, 
        DATEVALUE(LotRef1__c), 
        IF  (MID( TRIM(LotRef1__c) ,6, 2)="12", 
            DATEVALUE(LEFT( LotRef1__c , 4) &amp;"-12-31"), 
            DATEVALUE(LEFT( LotRef1__c , 4) &amp;"-" &amp; TEXT(VALUE(MID( LotRef1__c ,6,2))+1)   &amp;  "-01") -1)
        )
     )</formula>
    <label>Expiration Date</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Date</type>
  </fields>
  <fields>
    <fullName>Expiration_Flag__c</fullName>
    <externalId>false</externalId>
    <formula>IF( ISBLANK( LotRef1__c ) , "", IF( TODAY() &gt; Expiration_Date__c , IMAGE("/servlet/servlet.FileDownload?file=01520000000yzK6", "Red Flag"),  IF((TODAY() +548)  &gt;=  Expiration_Date__c, IMAGE("/servlet/servlet.FileDownload?file=01520000000zblq", "Yellow Flag"),  IMAGE("/servlet/servlet.FileDownload?file=01520000000yzK1","Green Flag.")) )
) 


/*
IF( TODAY() &gt; Expiration_Date__c , IMAGE("https://emea.salesforce.com/servlet/servlet.FileDownload?file=01520000000yzK6", "Red Flag"),  IF((TODAY() +90)  &gt;=  Expiration_Date__c, IMAGE("https://emea.salesforce.com/servlet/servlet.FileDownload?file=01520000000zblq", "Yellow Flag"),  IMAGE("https://emea.salesforce.com/servlet/servlet.FileDownload?file=01520000000yzK1","Green Flag.")) )

*/</formula>
    <label>Expiration Flag</label>
    <required>false</required>
