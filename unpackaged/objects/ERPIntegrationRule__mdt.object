  <validationRules>
    <fullName>CompanyCodePicklistIsNotTooLong</fullName>
    <active>true</active>
    <description>Ensures that records cannot be created that could break the integration.</description>
    <errorConditionFormula>LEN(TEXT(CompanyCode__c)) &gt; 20</errorConditionFormula>
    <errorDisplayField>CompanyCode__c</errorDisplayField>
    <errorMessage>The API value of the item picked in the Company Code picklist is too long to be valid.</errorMessage>
  </validationRules>
  <validationRules>
    <fullName>ErpIdMustBeNumeric</fullName>
    <active>true</active>
    <description>Ensures that records cannot be created that could break the integration.</description>
    <errorConditionFormula>NOT(ISNUMBER(TEXT(SourceIdERP__c)))</errorConditionFormula>
    <errorDisplayField>SourceIdERP__c</errorDisplayField>
