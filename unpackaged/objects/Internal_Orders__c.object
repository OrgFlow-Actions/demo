  <fields>
    <fullName>Invoice_Month__c</fullName>
    <externalId>false</externalId>
    <formula>IF ((MONTH ( Invoice_Date__c  ) = 1 ) , "01 Jan",
IF ((MONTH ( Invoice_Date__c  ) = 2 ) , "02 Feb",
IF ((MONTH ( Invoice_Date__c  ) = 3 ) , "03 Mar",
IF ((MONTH ( Invoice_Date__c  ) = 4 ) , "04 Apr",
IF ((MONTH ( Invoice_Date__c  ) = 5 ) , "05 May",
IF ((MONTH ( Invoice_Date__c  ) = 6 ) , "06 Jun",
IF ((MONTH ( Invoice_Date__c  ) = 7 ) , "07 Jul",
IF ((MONTH ( Invoice_Date__c  ) = 8 ) , "08 Aug",
IF ((MONTH ( Invoice_Date__c  ) = 9 ) , "09 Sep",
IF ((MONTH ( Invoice_Date__c  ) = 10 ) , "10 Oct",
IF ((MONTH ( Invoice_Date__c  ) = 11 ) , "11 Nov",
IF ((MONTH ( Invoice_Date__c  ) = 12 ) , "12 Dec", " "))))))))))))</formula>
    <label>Invoice Month</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
