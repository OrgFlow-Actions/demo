  <fields>
    <fullName>Internal_Order_for_analysis_only__c</fullName>
    <description>For analysis only, should not appear in any page layouts or search criterias if you are not a System administrator. The number 1 should appear if the order type is marked as an internal order type, else 0</description>
    <externalId>false</externalId>
    <formula>IF(AND
( OR
  (CONTAINS
   ( Order_Type__c, "7"
   ),CONTAINS
   ( Order_Type__c, "8"
   ),CONTAINS
   ( Order_Type__c, "9"
   ),CONTAINS
   ( Order_Type__c, "F"
   ),CONTAINS
   ( Order_Type__c, "P"
   ),CONTAINS
   ( Order_Type__c, "Q"
   ),CONTAINS
   ( Order_Type__c, "R"
   ),CONTAINS
   ( Order_Type__c, "S"
   ),CONTAINS
   ( Order_Type__c, "T"
   ),CONTAINS
   ( Order_Type__c, "SQZ"
   ),CONTAINS
   ( Order_Type__c, "SQG"
   )
  ),  

NOT
  ( OR
   ( CONTAINS
    (Order_Type__c, "0"
    ),CONTAINS
    (Order_Type__c, "1"
    ),CONTAINS
    (Order_Type__c, "2"
    ),CONTAINS
    (Order_Type__c, "3"
    ),CONTAINS
    (Order_Type__c, "4"
    )
   )
  ) 
,OR(Order_Type__c="AUR",Order_Type__c="ES8",Order_Type__c="ES9",Order_Type__c="ESR",Order_Type__c="GB7",Order_Type__c="GB9",Order_Type__c="GBR",Order_Type__c="GBT",Order_Type__c="NL7",Order_Type__c="NL8",Order_Type__c="NLR",Order_Type__c="NO8",Order_Type__c="NO9",Order_Type__c="NOS",Order_Type__c="NOT",Order_Type__c="PL7",Order_Type__c="PLR",Order_Type__c="PLS",Order_Type__c="PT8",Order_Type__c="SE7",Order_Type__c="SE8",Order_Type__c="SE9",Order_Type__c="SER",Order_Type__c="US7")),1,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>For analysis only, should not appear in any page layouts or search criterias if you are not a System administrator. The number 1 should appear if the order type is marked as an internal order type, else 0</inlineHelpText>
    <label>Internal Order? (for analysis only)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
  </fields>
