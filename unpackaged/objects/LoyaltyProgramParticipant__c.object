  <validationRules>
    <fullName>VR_DigitalWorkflow</fullName>
    <active>true</active>
    <description>Prevents activation if a field in the Digital Workflow section has not been populated.</description>
    <errorConditionFormula>AND(RecordType.DeveloperName = 'Loyalty_Program'
  ,ISPICKVAL(Account__r.Country__c,"CA")
  ,ISPICKVAL(Status__c,"Active")
  ,OR(
  	ISPICKVAL(LoyaltyProgram__r.Subtype__c,"One DS")
    ,ISPICKVAL(LoyaltyProgram__r.Subtype__c,"One DS Pro")
    ,ISPICKVAL(LoyaltyProgram__r.Subtype__c,"One DSO")
  )
  ,OR(
    ISBLANK(TEXT(AdvancedProfiling__r.GuidedImplantPlacement__c))
    ,ISBLANK(TEXT(AdvancedProfiling__r.ImplantRestoration__c))
    ,ISBLANK(TEXT(AdvancedProfiling__r.Crowns__c))
