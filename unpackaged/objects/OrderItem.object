  <fields>
    <fullName>Discount_Approval_Eval__c</fullName>
    <externalId>false</externalId>
    <formula>IF( AND(Discount_Percent__c  &gt; 0, (LEN( Text(Sales_Special__c))&gt;0)),"NO APPROVAL NEEDED", 

IF(AND(Discount_Percent__c  &gt; 0,(LEN( Text(Sales_Special__c))=0)), "YES - APPROVAL NEEDED",

IF(Discount_Percent__c  &gt; 0, "YES - APPROVAL NEEDED",
IF(Discount_Percent__c   = 0, "NO APPROVAL NEEDED",

"YES"))))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Discount Approval Evaluation</label>
