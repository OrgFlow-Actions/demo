  <validationRules>
    <fullName>Can_Not_Pick_Header_Specials</fullName>
    <active>true</active>
    <errorConditionFormula>OR( Text(Sales_Special__c)= "***DENTSPLY Implants Specials***",
Text(Sales_Special__c) ="***National Specials***")</errorConditionFormula>
    <errorDisplayField>Sales_Special__c</errorDisplayField>
    <errorMessage>Please pick a valid sales special from the list (not the header row!)</errorMessage>
  </validationRules>
  <validationRules>
    <fullName>Check_if_discount_100</fullName>
    <active>false</active>
    <errorConditionFormula>IF(AND(Discount_Percent__c =1.00, ISPICKVAL(Free_of_Charge_Delivery__c, ""), OR(Order.RecordTypeId ="0120J000000cn2w", Order.RecordTypeId ="0120J000000cn31")), true, false)</errorConditionFormula>
    <errorDisplayField>Free_of_Charge_Delivery__c</errorDisplayField>
    <errorMessage>You must choose a type of delivery for products that are free of charge</errorMessage>
  </validationRules>
  <validationRules>
    <fullName>Check_if_foc_notblank</fullName>
    <active>false</active>
    <errorConditionFormula>AND(or(isblank(Discount_Percent__c), Discount_Percent__c&lt;1), Isblank(text(Free_of_Charge_Delivery__c))=False, OR(Order.RecordTypeId ="0120J000000cn2w", Order.RecordTypeId ="0120J000000cn31"))</errorConditionFormula>
    <errorDisplayField>Discount_Percent__c</errorDisplayField>
    <errorMessage>You must insert 100% discount if the products are free of charge</errorMessage>
  </validationRules>
  <validationRules>
    <fullName>OM_Max_Discount</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Discount_Percent__c &gt; 1, Discount_Percent__c &lt; 0)</errorConditionFormula>
    <errorDisplayField>Discount_Percent__c</errorDisplayField>
    <errorMessage>Discount Percent must be between 0 and 100%</errorMessage>
  </validationRules>
  <validationRules>
    <fullName>User_May_Not_Change_the_Unit_Price</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
ISCHANGED(UnitPrice),
$User.ProfileId &lt;&gt; "00e20000001Ygwu", /*#System: System Administrator*/
$User.ProfileId &lt;&gt; "00e0J000001cy8i", /*#System: System Administrator (CPQ)*/
$User.ProfileId &lt;&gt; "00e20000000kVxk",  /*System Administrator*/
$User.ProfileId &lt;&gt;  "00e0J000001Ar8G",  /*#System: Integration User*/
$User.ProfileId &lt;&gt;  "00e6M000001Yjg8" /*#System: All Integration Users*/)</errorConditionFormula>
    <errorDisplayField>UnitPrice</errorDisplayField>
    <errorMessage>You are not allowed to change the Unit Price, since it is controlled via the price list in Salesforce.com. To change the price, please add a discount instead.</errorMessage>
  </validationRules>
