IF( (Country_Code__c ='DE'|| Country_Code__c ='US'||Country_Code__c ='PL'||Country_Code__c ='CA' )&amp;&amp;Value(Invoice_Year__c)&gt;=2017,'2017','') &amp;



"&amp;view=A&amp;selectionbox_Orderno=" &amp;

Order_Number__c &amp;


"&amp;selectionbox_Division=" &amp;

IF( (Country_Code__c &lt;&gt;'DE' &amp;&amp; Country_Code__c &lt;&gt;'US'&amp;&amp; Country_Code__c &lt;&gt;'CA' ),'',Country_Code__c) &amp;


"&amp;selectionbox_Invoiceno=" &amp;

IF( (Country_Code__c &lt;&gt;'DE' &amp;&amp; Country_Code__c &lt;&gt;'US' &amp;&amp; Country_Code__c &lt;&gt;'PL' &amp;&amp; Country_Code__c &lt;&gt;'CA' ),'',Invoice_Number__c) &amp;


"&amp;view=A"
,

 $Label.View_Invoice 

,

"_blank"
)</formula>
    <inlineHelpText>View invoice in Document Archive (within Dentsply domain or via VPN only)</inlineHelpText>
