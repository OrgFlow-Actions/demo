  <fields>
    <fullName>Abutment_Implant_System__c</fullName>
    <externalId>false</externalId>
    <formula>IF (CONTAINS(Product__r.RecordType.DeveloperName, "Global") ,IF(
Product__r.PG2_Code__c ="ID21" ||Product__r.PG2_Code__c ="ID22" || Product__r.PG2_Code__c ="ID23", 
/* checks if this is Atlantis */  

CASE( Implant_system__c, 
"Dentsply Friadent", LEFT( Atlantis_Interface_D__c , FIND( " ", Atlantis_Interface_D__c)),
"Astra Tech", "Astra Tech Implant System",
"Astra Tech (D)","Astra Tech Implant System",
"Ankylos (D)","Ankylos",
"XiVE/Frialit (D)","XiVE",
Implant_system__c) ,
/* If Atlantis, check if manufacturer is Friadent and add the first word in the implant interface (Ankylos, Frialit, XiVe). if Astra Tech, change to Astra Tech Implant System to match Implant brand, other - add implant manufacturer */ 

IF(Product__r.PG2_Code__c ="II12", Brand__c ,"") ) 
/*If not Atlantis check if Stock Abutments. If so Add Brand, if not leave blank */
 , 
 IF(

Product__r.GlobalSKUMapping__r.PG2_Code__c ="ID21" ||Product__r.GlobalSKUMapping__r.PG2_Code__c ="ID22" || Product__r.GlobalSKUMapping__r.PG2_Code__c ="ID23", 
/* checks if this is Atlantis */  

CASE( Implant_system__c, 
"Dentsply Friadent", LEFT( Atlantis_Interface_D__c , FIND( " ", Atlantis_Interface_D__c)),
"Astra Tech", "Astra Tech Implant System",
"Astra Tech (D)","Astra Tech Implant System",
"Ankylos (D)","Ankylos",
"XiVE/Frialit (D)","XiVE",
Implant_system__c) ,
/* If Atlantis, check if manufacturer is Friadent and add the first word in the implant interface (Ankylos, Frialit, XiVe). if Astra Tech, change to Astra Tech Implant System to match Implant brand, other - add implant manufacturer */ 

IF(Product__r.GlobalSKUMapping__r.PG2_Code__c ="II12", Brand__c ,"") ) )
/*If not Atlantis check if Stock Abutments. If so Add Brand, if not leave blank */</formula>
    <label>Abutment Implant System</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
