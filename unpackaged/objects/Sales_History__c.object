    <description>View invoice in Document Archive (within Dentsply domain or via VPN only)</description>
    <externalId>false</externalId>
    <formula>/* HYPERLINK("http://a3.astratech.net/a3DirectStart.asp?COMPONENT=OpenCollector_Invoice_0_Collector&amp;view=A&amp;selectionbox_Invoiceno="  &amp; Invoice_Number__c  &amp; "&amp;selectionbox_Orderno=" &amp;  Order_Number__c &amp;"&amp;View=A", "View invoice","_blank")*/

