    <externalId>false</externalId>
    <formula>IF(AND(BEGINS(Product_Group_2__c, "ID2") ,NOT( ISNULL( Order_Number__c ) )) , MID( Order_Number__c , 4,  LEN( Order_Number__c ) -3) , NULL)</formula>
