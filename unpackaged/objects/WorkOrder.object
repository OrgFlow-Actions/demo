  <validationRules>
    <fullName>Status_equals_closed</fullName>
    <active>true</active>
    <description>This validation rule will be used to prevent partner users marking the status as Closed, however they should only be able to complete it.</description>
    <errorConditionFormula>AND(
				 OwnerId &lt;&gt; $User.Id,
     ISCHANGED(Status),
     ISPICKVAL(Status, "Closed")
   )</errorConditionFormula>
    <errorMessage>Only work order owner can close this</errorMessage>
  </validationRules>
